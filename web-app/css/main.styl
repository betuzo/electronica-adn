*
  margin: 0
  padding: 0

body,

html.no-cssgradients
  background-color: #aaa

.ie6 html
  height: 100%

body
  margin: 0
  padding: 0
  background: url(../images/img01.gif) repeat-x left top
  font-size: 16px
  text-align: justify
  color: #333333
  font-family: Tahoma

#header
  width: 95%
  margin: 0 auto
  height: 42px
  text-align: center

#menu
  border-radius: .5em
  margin: 0 auto
  width: 960px
  height: 50px
  background: url(../images/header-wrapper.svg) no-repeat left top

#nav
  display: inline-block
  width: 95%
  margin: 0 auto
  vertical-align: middle
  text-align: left
  font-family: arial, Helvetica

ul#navigation
  margin: 0px auto
  text-align: right

ul#navigation li
  display: inline-block
  font-size: 13px
  font-weight: bold
  margin: 0
  padding: 0
  float: left
  position: relative

ul#navigation li a
  padding: 13px 25px
  color: #fff
  text-decoration: none
  display: inline-block
  border-right: 2px solid #007294
  vertical-align: top
  background-color: #007294
  background-image: linear-gradient(top, #008dab, #007294)
  border-radius: .4em

ul#navigation li a:hover
  background: #007294
  color: #fff

ul#navigation li:hover > a
  background: #007294

ul#navigation li:hover > ul
  visibility: visible
  opacity: .9

ul#navigation ul, ul#navigation ul li ul
  list-style: none
  margin: 0
  padding: 0
  visibility: hidden
  position: absolute
  z-index: 99999
  width: 180px
  background: #f8f8f8
  box-shadow: 1px 1px 3px #ccc
  border-radius: .4em
  opacity: 0

ul#navigation ul
  top: 40px

ul#navigation ul li ul
  top: 0
  left: 181px

ul#navigation ul li
  clear: both
  width: 100%
  border-bottom: 1px solid #c9c9c9

ul#navigation ul li a
  background: none
  padding: 7px 15px
  color: #888888
  text-decoration: none
  display: inline-block
  border: 0 none
  float: left
  clear: both
  width: 150px

ul#navigation li a.first
  border-left: 0 none

ul#navigation li a.last
  border-right: 0 none

#container
  margin: .4em auto

#page
  width: 960px
  margin: 0 auto
  background: #fff
  padding: 0 25px

#form-pagos
  margin: .5em auto
  border-radius: .3em
  border: .1em solid #808080
  width: 500px

#form-fecha
  margin: .5em auto
  border-radius: 1em
  border: .3em solid #007294
  width: 500px

#form-refacciones
  margin: .2em auto
  border-radius: .1em
  border: .1em solid #808080
  width: auto

#form-telefono-proveedor
  margin: .5em auto
  border-radius: 1em
  border: .3em solid #007294
  padding: .5em
  width: 400px

.label-refacciones
  cursor: pointer
  display: inline-block
  margin: 0 0.25em 0 0
  color: #000

.ie6 body,
.ie7 body,
.ie8 body
  border-color: #255b17
  border-style: solid
  border-width: 0 1px

.ie6 body
  height: 100%

a:link, a:visited, a:hover
  color: #2c6a80
  text-decoration none

a:hover, a:active
  outline: none
  color: #0A7DA5
  font-weight bold

h1
  color: #2c6a80
  font-weight: normal
  font-size: 1.25em
  margin: 0.8em 0 0.3em 0

ul
  padding: 0

img
  border: 0

#grailsLogo
  width: 960px
  margin: 1em auto
  text-align: left

#grailsLogo a
  display: block

#img-main
  margin: 1em
  text-align: center

#img-main img
  border-radius: 0.5em

.content

.content h1
  border-bottom: 1px solid #CCCCCC
  margin: 1em 0 1em 0
  padding: 0
  font-size: 1em

.scaffold-list h1
  border: none

.footer
  color: #fff
  clear: both
  font-size: 0.8em
  margin-top: 1.5em
  padding: 1em
  min-height: 1em
  background-image: linear-gradient(top, #008dab, #007294)
  border-radius: .4em

.footer a
  color: #255b17

.spinner
  background: url(../images/spinner.gif) 50% 50% no-repeat transparent
  height: 16px
  width: 16px
  padding: 0.5em
  position: absolute
  right: 0
  top: 0
  text-indent: -9999px

.nav
  padding: 0.2em
  zoom: 1
  font-family: Tahoma
  font-size: 10pt

.nav ul
  overflow: hidden
  padding-left: 0
  zoom: 1

.nav li
  display: block
  float: left
  list-style-type: none
  margin-right: 0.5em
  padding: 0

.nav a
  color: #666666
  font-weight: bold
  display: block
  padding: 0.25em 0.7em
  text-decoration: none
  -moz-border-radius: 0.3em
  -webkit-border-radius: 0.3em
  border-radius: 0.3em

.nav a:active, .nav a:visited
  color: #413E3E

.nav a:focus, .nav a:hover
  background-color: #999999
  color: #ffffff
  outline: none
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.8)

.no-borderradius .nav a:focus, .no-borderradius .nav a:hover
  background-color: transparent
  color: #444444
  text-decoration: underline

.nav a.home, .nav a.list, .nav a.create
  background-position: 0.7em center
  background-repeat: no-repeat
  text-indent: 25px

.nav a.home
  background-image: url(../images/skin/house.png)

.nav a.list
  background-image: url(../images/skin/database_table.png)

.nav a.create
  background-image: url(../images/skin/database_add.png)

.icon-grid
  display: inline-block
  font-family: Tahoma
  font-size: 9pt

fieldset
  border none

.property-list
  margin: 0.6em 1.25em 0 1.25em
  padding: 0.3em 1.8em 1.25em
  position: relative
  zoom: 1
  border: none

.property-list .fieldcontain
  list-style: none
  overflow: hidden
  zoom: 1

.fieldcontain
  margin-top: 1em

.fieldcontain .property-label
  color: #666666
  text-align: right
  width: 25%

.fieldcontain label
  width 90%
  color: #666666
  text-align: left

.fieldcontain .property-label
  float: left

.fieldcontain .property-value
  display: block
  margin-left: 27%

.fieldcontain .property-value-open
  display: block
  background: #D1ECB6
  font-size: 2em
  text-align: center

.fieldcontain .property-value-close
  display: block
  background: #ECB6B6
  font-size: 2em
  text-align: center

label
  cursor: pointer
  display: inline-block
  margin: 0 0.25em 0 0

.required-indicator
  color: #57adc7
  display: inline-block
  font-weight: bold
  margin-left: 0.3em
  position: relative
  top: 0.1em

ul.one-to-many
  display: inline-block
  list-style-position: inside
  vertical-align: top

.ie6 ul.one-to-many, .ie7 ul.one-to-many
  display: inline
  zoom: 1

ul.one-to-many li.add
  list-style-type: none

fieldset.embedded
  background-color: transparent
  border: 1px solid #CCCCCC
  padding-left: 0
  padding-right: 0
  -moz-box-shadow: none
  -webkit-box-shadow: none
  box-shadow: none

fieldset.embedded legend
  margin: 0 1em

.errors,
.message
  font-size: 0.8em
  line-height: 2
  margin: 1em 2em
  padding: 0.5em

.message
  background: #f3f3ff
  border: 1px solid #b2d1ff
  color: #006dba
  -moz-box-shadow: 0 0 0.25em #b2d1ff
  -webkit-box-shadow: 0 0 0.25em #b2d1ff
  box-shadow: 0 0 0.25em #b2d1ff

.errors
  background: #fff3f3
  border: 1px solid #ffaaaa
  color: #cc0000
  -moz-box-shadow: 0 0 0.25em #ff8888
  -webkit-box-shadow: 0 0 0.25em #ff8888
  box-shadow: 0 0 0.25em #ff8888

.errors ul,
.message
  padding: 0

.errors li
  list-style: none
  background: transparent url(../images/skin/exclamation.png) 0 50% no-repeat
  text-indent: 22px

.message
  background: transparent url(../images/skin/information.png) 0 50% no-repeat
  text-indent: 22px


table
  border: 1px solid #DFDFDF
  border-collapse: collapse
  width: 100%

tr
  border: 0

tr>td:first-child, tr>th:first-child
  padding-left: 1.25em

tr>td:last-child, tr>th:last-child
  padding-right: 1.25em

td, th
  line-height: 1.5em
  padding: 0.5em 0.6em
  text-align: left
  vertical-align: top

th
  background-color: #efefef
  // // background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #ffffff), color-stop(1, #eaeaea))
  // filter: progid:DXImageTransform.Microsoft.gradient(startColorStr = '#ffffff', EndColorStr = '#eaeaea')
  // -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#ffffff', EndColorStr='#eaeaea')"
  color: #666666
  font-weight: bold
  line-height: 1.7em
  padding: 0.2em 0.6em

thead th
  white-space: nowrap

th a
  display: block
  text-decoration: none

th a:link, th a:visited
  color: #666666

th a:hover, th a:focus
  color: #333333

th.sortable a
  background-position: right
  background-repeat: no-repeat
  padding-right: 1.1em

th.asc a
  background-image: url(../images/skin/sorted_asc.gif)

th.desc a
  background-image: url(../images/skin/sorted_desc.gif)

.odd
  background: #f7f7f7

.even
  background: #ffffff

.ordenOpen
  background: #D1ECB6

.ordenClose
  background: #ECB6B6

th:hover, tr:hover
  background: #b6e7f1

.buttons
  padding: 0.3em
  margin: 1em 0 0 0
  border 0

.buttonsGrid
  background-color: #fff
  overflow: hidden
  padding: 0.3em
  margin: 0.1em 0 0 0
  border 0

.buttonsGrid div,
.buttons input,
.buttons div,
.buttons a
  background-color: transparent
  border: 0
  color: #666666
  cursor: pointer
  display: inline-block
  margin: 0 0.25em 0
  overflow: visible
  padding: 0.25em 0.7em
  text-decoration: none
  -moz-border-radius: 0.3em
  -webkit-border-radius: 0.3em
  border-radius: 0.3em

.buttons input:hover, .buttons input:focus,
.buttons div:hover, .buttons div:focus,
.buttons a:hover, .buttons a:focus, .buttonsGrid div:hover
  background-color: #999999
  color: #ffffff
  outline: none
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.8)
  -moz-box-shadow: none
  -webkit-box-shadow: none
  box-shadow: none

.no-borderradius .buttons input:hover, .no-borderradius .buttons input:focus,
.no-borderradius .buttons a:hover, .no-borderradius .buttons a:focus
  background-color: transparent
  color: #444444
  text-decoration: underline

.buttons .delete, .buttons .edit, .buttons .save, .buttons .search, .buttons .close, .buttons .ready, .buttons .closeIcon , .buttons .saveIcon, .buttonsGrid .searchIcon, .buttonsGrid .closeIcon
  background-position: 0.7em center
  background-repeat: no-repeat
  text-indent: 25px

.buttons .delete
  background-image: url(../images/skin/database_delete.png)

.buttons .edit
  background-image: url(../images/skin/database_edit.png)

.buttons .save
  background-image: url(../images/skin/database_save.png)

.buttons .closeIcon, .buttonsGrid .closeIcon
  background-image: url(../images/cerrar.png)
  background-size: 25px 25px

.buttonsGrid .searchIcon
  background-image: url(../images/Search.png)
  background-size: 25px 25px

.buttons .saveIcon
  background-image: url(../images/Floppy.png)
  background-size: 25px 25px

.buttons .search
  background-image: url(../images/skin/search.gif)

.buttons .close
  background-image: url(../images/skin/close.gif)

.buttons .ready
  background-image: url(../images/skin/ready.gif)

a.skip
  position: absolute
  left: -9999px

#guardarajax
  border: 0px
  background: url(../images/Floppy.png) no-repeat

.btnGuardar
  background-image: url(../images/Floppy.png)
  background-size: 80px 60px
  background-repeat: no-repeat

#slide-fecha-open:hover
  background-size: 80px 60px

#overlay
  background: #fff
  position: absolute
  left: 330px
  top: 500px
  visibility: hidden
  min-width: 40%
  border: .1em solid #808080
  margin: .5em auto
  border-radius: .3em
  -moz-border-radius: .3em
  -webkit-border-radius: .3em
  box-shadow: 0 0 50px #000
  -moz-box-shadow: 0 0 50px #000
  -webkit-box-shadow: 0 0 50px #000

#overlay div#overlayContainer
  background-color: #fff
  position: relative
  padding: 15px
  text-align: left
  -moz-border-radius: 15px
  -webkit-border-radius: 15px
  -moz-box-shadow: 0 0 50px #000
  -webkit-box-shadow: 0 0 50px #000

#modal-refacciones
  background: #000
  position: absolute
  left: 330px
  top: 370px
  visibility: hidden
  min-width: 40%
  text-align: left
  border: 0.2 solid #333
  opacity: .9
  -moz-border-radius: 15px
  -webkit-border-radius: 15px
  -moz-box-shadow: 0 0 50px #000
  -webkit-box-shadow: 0 0 50px #000

#modal-refacciones div#overlayContainer
  background-color: #fff
  position: relative
  padding: 15px
  text-align: left
  -moz-border-radius: 15px
  -webkit-border-radius: 15px
  -moz-box-shadow: 0 0 50px #000
  -webkit-box-shadow: 0 0 50px #000

.vform
  width: 100%
  max-width: 360px

.hform
  width: 100%
  max-width: 420px

.vform, .hform
  margin-bottom: 20px
  font: 14px "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, sans-serif
  color: #333

.vform label
  width: 100%

.hform label
  width: 40%

